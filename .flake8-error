[flake8]
count = True
select =
    # Runtime errors (Syntax, Indentation, IO)
    E9,
    # Dictionary key name repeated
    F60,
    # Expression-unpacking-related
    F62,
    # Wild statements
    F70,
    # Syntax error in documentation
    F72,
    # Undefined names
    F82,
    # Duplicated argument names
    F83
max-complexity = 10
max-line-length = 119
show-source = True
statistics = True